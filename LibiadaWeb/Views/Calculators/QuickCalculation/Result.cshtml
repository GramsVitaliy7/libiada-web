@model dynamic

@{
    ViewBag.Title = "Характеристики произвольной цепочки";
}

<h2>Характеристики произвольной цепочки</h2>
@if (ViewBag.Error == null)
{
    <table class="table table-bordered table-hover" id="resultTable">
        @for (int i = 0; i < ViewBag.characteristics.Count; i++)
        {
            <tr>
                <td>
                    @ViewBag.characteristicNames[i]
                </td>
                <td>
                    @ViewBag.characteristics[i]
                </td>
            </tr>
        }
    </table>
}

