@model string


@Scripts.Render("~/bundles/angularjs")
@Scripts.Render("~/bundles/functions")
@Scripts.Render("~/bundles/controllers/" + @Model)



<script type="text/javascript">
    "use strict";

    var data = @Html.Raw(ViewBag.data);
    var controllerConstructor = @Model;
    controllerConstructor(data);
</script>
