@{
    ViewBag.Title = "Sequence import result";
}

<h2>@ViewBag.Title</h2>

@if (ViewBag.Error == null)
{
    @Html.Partial("_AngularControllerInitializer", "BatchSequenceImportResultController")

    <div ng-app="BatchSequenceImportResult" ng-controller="BatchSequenceImportResultCtrl">
        @Html.Partial("_ModalLoadingWindow")
        <br />
        <div>
            Sequence <strong> {{matterName}} </strong> imported successfully
        </div>
        <br />
        <div>
            @Html.ActionLink("Import another sequence", "Create") or go @Html.ActionLink("back to List", "Index").
        </div>
    </div>
}
