@model dynamic

@{
    ViewBag.Title = "Проверка цепочек";
}

<h2>Проверка цепочек</h2>
@using (Html.BeginForm("Index", "SequenceCheck", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Цепочка</legend>

        <div class="editor-label">
            @Html.Label("цепочка")
        </div>
        <div class="editor-field">
            @Html.DropDownList("matterId", string.Empty)
        </div>

        <input type="file" name="file" id="file"/>

        <p>
            <input type="submit" value="Проверить"/>
        </p>
    </fieldset>
}
