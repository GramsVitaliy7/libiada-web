@{
    ViewBag.Title = "Annotations check";
}

@Html.Partial("_AngularControllerInitializer")

<h2>@ViewBag.Title</h2>

@using (Html.BeginForm("Index", "AnnotationsCheck", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal" ng-app="GenesImport" ng-controller="GenesImportCtrl">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.Partial("_LocalFile")
        @Html.Partial("_MattersTable")
        @Html.Partial("_MattersSubmit")
    </div>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
